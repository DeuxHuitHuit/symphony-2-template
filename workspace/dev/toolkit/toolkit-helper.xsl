	<xsl:template name="deref-value">
		<xsl:param name="value" />
		<xsl:choose>
			<xsl:when test="contains($value, '@')">
				<xsl:variable name="tokens" select="str:tokenize($value)" />
				<xsl:variable name="unref-var">
					<xsl:for-each select="$tokens">
						<!-- Find real value -->
						<xsl:choose>
							<xsl:when test="starts-with(., '@spacing-')">
								<xsl:variable name="node-name" select="substring(., 10)" />
								<val>
									<xsl:value-of select="$theme-node/spacing/*[name() = $node-name]/@value" />
								</val>
							</xsl:when>
							<xsl:when test="starts-with(., '@dimension-')">
								<xsl:variable name="node-name" select="substring(., 12)" />
								<val>
									<xsl:copy-of select="$theme-node/dimensions/*[name() = $node-name]/@value" />
								</val>
							</xsl:when>
							<xsl:otherwise>
								<val>
									<xsl:copy-of select="." />
								</val>
							</xsl:otherwise>
						</xsl:choose>
					</xsl:for-each>
				</xsl:variable>
				<xsl:copy-of select="$unref-var" />
			</xsl:when>
			<xsl:otherwise>
				<val>
					<xsl:value-of select="$value" />
				</val>
			</xsl:otherwise>
		</xsl:choose>
	</xsl:template>
</xsl:stylesheet>