<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0"
	xmlns:exsl="http://exslt.org/common"
	xmlns:str="http://exslt.org/strings"
	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
	exclude-result-prefixes="exsl str">

	<xsl:template name="toolkit-article">
		<xsl:param name="name" />
		<xsl:param name="attr" />
		<xsl:param name="attr-title" />
		<xsl:param name="title" select="$name"/>
		<xsl:param name="content" />

		<xsl:variable name="c-attr">
			<add id="{$name}" />
			<add style="padding-top: 10rem;" />
			<xsl:copy-of select="$attr" />
		</xsl:variable>

		<xsl:variable name="c-attr-title" >
			<xsl:copy-of select="$attr-title" />
		</xsl:variable>

		<xsl:call-template name="element">
			<xsl:with-param name="attr" select="$c-attr" />
			<xsl:with-param name="element" select="'article'"/>
			<xsl:with-param name="content" >
				<header class="toolkit-article-header">
					<xsl:call-template name="heading">
						<xsl:with-param name="attr" select="c-attr-title"/>
						<xsl:with-param name="content" select="$title"/>
					</xsl:call-template>
				</header>

				<xsl:call-template name="content">
					<xsl:with-param name="content" select="$content"/>
				</xsl:call-template>
			</xsl:with-param>
		</xsl:call-template>
	</xsl:template>

	<xsl:template name="main-section">
		<xsl:param name="name" select="''" />
		<xsl:param name="content" />
		<xsl:param name="attr" />

		<xsl:variable name="c-attr" >
			<xsl:copy-of select="$attr" />
		</xsl:variable>

		<xsl:if test="(string-length($name) != 0 and /data/params/url-page = $name) or (string-length($name) = 0 and string-length(/data/params/url-page) = 0)">
			<xsl:call-template name="element">
				<xsl:with-param name="attr" select="$c-attr"/>
				<xsl:with-param name="content" select="$content" />
			</xsl:call-template>
		</xsl:if>
	</xsl:template>

